{
  "name": "core-service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/app",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "dev": "nodemon --exec \"ts-node --files -r dotenv/config\" --verbose --watch src --ext ts,json src/app",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.test.json",
    "test": "cross-env NODE_ENV=test mocha -b -c --timeout 10000 --extension spec.js --recursive --exit \"build/$npm_config_files\"",
    "test:ts": "cross-env NODE_ENV=test mocha -r ts-node/register -b -c --diff --timeout 10000 --extension spec.ts --recursive --exit \"src/$npm_config_files\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "npm run lint -- --fix"
  },
  "license": "NOLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinedefi/core-service.git"
  },
  "bugs": {
    "url": "https://github.com/pinedefi/core-service/issues"
  },
  "homepage": "https://github.com/pinedefi/core-service#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-typescript": "^7.16.7",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@andrewscwei/super-error": "^1.7.0",
    "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
    "@solana/web3.js": "^1.37.1",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "web3": "^1.7.1",
    "winston": "^3.7.2"
  }
}
