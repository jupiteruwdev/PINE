{
  "name": "core-service",
  "version": "1.0.0",
  "description": " Core web service and API gateway",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings --experimental-specifier-resolution node build/app",
    "clean": "rimraf build",
    "lint-staged": "lint-staged",
    "prebuild": "npm run lint",
    "dev": "nodemon --exec \"node --no-warnings --experimental-specifier-resolution node --loader ts-node/esm -r dotenv/config\" --verbose --watch src --ext ts,json src/app",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.test.json",
    "test": "npm run test:unit && npm run test:int",
    "test:unit": "cross-env NODE_ENV=test mocha --config .mocharc --extension spec.js \"build/$npm_config_files\"",
    "test:int": "npm run test:int:ts",
    "test:ts": "npm run test:unit:ts && npm run test:int:ts",
    "test:unit:ts": "cross-env NODE_ENV=test mocha --config .mocharc -r ts-node/register -n loader=ts-node/esm --extension spec.ts \"src/$npm_config_files\"",
    "test:int:ts": "cross-env NODE_ENV=test mocha --config .mocharc -r ts-node/register -n loader=ts-node/esm --extension ts \"tests/$npm_config_files\"",
    "ship:start": "docker-compose -f docker-compose.yml up --build",
    "ship:test": "docker-compose -f docker-compose.test.yml up --build --exit-code-from app",
    "lint": "eslint --ext .ts --ignore-pattern entities --ignore-pattern db src",
    "lint:fix": "npm run lint -- --fix",
    "test:ci": "npm run test:unit:ci ; npm run test:int:ts:ci",
    "test:unit:ci": "cross-env NODE_ENV=test mocha --config .mocharc --extension spec.js \"build/$npm_config_files\" -R json -O output=test-results/unit-tests.json",
    "test:int:ts:ci": "cross-env NODE_ENV=test mocha --config .mocharc -r ts-node/register -n loader=ts-node/esm --extension ts \"tests/$npm_config_files\" -R json -O output=test-results/integration-tests.json"
  },
  "license": "NOLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinedefi/core-service.git"
  },
  "bugs": {
    "url": "https://github.com/pinedefi/core-service/issues"
  },
  "homepage": "https://github.com/pinedefi/core-service#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/keccak": "^3.0.1",
    "@types/lodash": "^4.14.183",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "lint-staged": "^13.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@andrewscwei/super-error": "^2.1.0",
    "@google-cloud/logging-winston": "^5.2.3",
    "@looksrare/sdk": "^0.10.4",
    "@metaplex-foundation/mpl-token-metadata": "^2.2.3",
    "@sentry/integrations": "^7.32.1",
    "@sentry/node": "^7.32.1",
    "@sentry/tracing": "^7.32.1",
    "@truffle/hdwallet-provider": "^2.1.7",
    "@types/ethereum-block-by-date": "^1.4.1",
    "@types/object-hash": "^2.2.1",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "ethereum-block-by-date": "^1.4.6",
    "ethers": "^5.7.2",
    "ethers-gcp-kms-signer": "^1.1.6",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "ip": "^1.1.8",
    "keccak": "^3.0.3",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "opensea-js": "^6.1.5",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.7",
    "tslib": "^2.4.1",
    "web3": "^1.7.5",
    "winston": "^3.8.1"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
