{
  "name": "core-service",
  "version": "1.0.0",
  "description": " Core web service and API gateway",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution node build/app",
    "clean": "rimraf build .tests",
    "prebuild": "npm run lint",
    "dev": "nodemon --exec \"node --experimental-specifier-resolution node --loader ts-node/esm -r dotenv/config\" --verbose --watch src --ext ts,json src/app",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.test.json",
    "test": "npm run test:unit && npm run test:int",
    "test:unit": "cross-env NODE_ENV=test MOCHAWESOME_REPORTFILENAME=unit mocha --config .mocharc --extension spec.js \"build/$npm_config_files\"",
    "test:int": "npm run test:int:ts",
    "test:ts": "npm run test:unit:ts && npm run test:int:ts",
    "test:unit:ts": "cross-env NODE_ENV=test MOCHAWESOME_REPORTFILENAME=unit mocha --config .mocharc -r ts-node/register -n loader=ts-node/esm --extension spec.ts \"src/$npm_config_files\"",
    "test:int:ts": "cross-env NODE_ENV=test MOCHAWESOME_REPORTFILENAME=integration mocha --config .mocharc -r ts-node/register -n loader=ts-node/esm --extension ts \"tests/$npm_config_files\"",
    "ship:start": "docker-compose -f docker-compose.yml up --build",
    "ship:test": "docker-compose -f docker-compose.test.yml up --build --exit-code-from app",
    "lint": "eslint --ext .ts --ignore-pattern entities src",
    "lint:fix": "npm run lint -- --fix"
  },
  "license": "NOLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinedefi/core-service.git"
  },
  "bugs": {
    "url": "https://github.com/pinedefi/core-service/issues"
  },
  "homepage": "https://github.com/pinedefi/core-service#readme",
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.183",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@andrewscwei/super-error": "^2.0.0",
    "@metaplex-foundation/mpl-token-metadata": "^2.2.3",
    "@solana/web3.js": "^1.53.0",
    "@types/object-hash": "^2.2.1",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql-request": "^4.3.0",
    "ip": "^1.1.8",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "web3": "^1.7.5",
    "winston": "^3.8.1"
  }
}
