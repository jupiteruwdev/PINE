{
  "name": "treasury-service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/app",
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "dev": "nodemon --exec \"ts-node --files -r dotenv/config\" --verbose --watch src --ext ts,json src/app",
    "build": "npm run clean && tsc",
    "build:test": "npm run clean && tsc --project tsconfig.test.json",
    "test": "mocha -r dotenv/config -b -c --timeout 10000 --extension spec.js --recursive --exit \"build/$npm_config_files\"",
    "test:ts": "mocha -r ts-node/register -b -c --diff --timeout 10000 --extension spec.ts --recursive --exit \"src/$npm_config_files\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "npm run lint -- --fix"
  },
  "license": "NOLICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinedefi/treasury-service.git"
  },
  "bugs": {
    "url": "https://github.com/pinedefi/treasury-service/issues"
  },
  "homepage": "https://github.com/pinedefi/treasury-service#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.6",
    "dotenv": "^14.3.2",
    "eslint": "^8.8.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "web3": "^1.6.0"
  }
}
